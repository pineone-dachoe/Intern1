<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 체크박스 해제 상태 -->
    <item android:state_checked="false"
        android:drawable="@drawable/ic_checkbox_b_off">
        <shape>
            <size
                android:height="10dp"
                android:width="10dp"
                />
        </shape>
    </item>
    <!-- 체크박스 선택 상태 -->
    <item android:state_checked="true"
        android:drawable="@drawable/ic_checkbox_b_on"
        >
        <shape>
            <size
                android:height="10dp"
                android:width="10dp"
                />
        </shape>
    </item>
</selector>